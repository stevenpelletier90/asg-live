{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#0073aa",
					"name": "Primary"
				},
				{
					"slug": "primary-dark",
					"color": "#005177",
					"name": "Primary Dark"
				},
				{
					"slug": "primary-light",
					"color": "#3399cc",
					"name": "Primary Light"
				},
				{
					"slug": "secondary",
					"color": "#23282d",
					"name": "Secondary"
				},
				{
					"slug": "secondary-light",
					"color": "#3c4349",
					"name": "Secondary Light"
				},
				{
					"slug": "accent",
					"color": "#00a0d2",
					"name": "Accent"
				},
				{
					"slug": "accent-dark",
					"color": "#0080a8",
					"name": "Accent Dark"
				},
				{
					"slug": "success",
					"color": "#46b450",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#ffb900",
					"name": "Warning"
				},
				{
					"slug": "error",
					"color": "#dc3232",
					"name": "Error"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "background-alt",
					"color": "#f7f7f7",
					"name": "Background Alt"
				},
				{
					"slug": "foreground",
					"color": "#333333",
					"name": "Foreground"
				},
				{
					"slug": "muted",
					"color": "#666666",
					"name": "Muted"
				},
				{
					"slug": "subtle",
					"color": "#999999",
					"name": "Subtle"
				},
				{
					"slug": "border",
					"color": "#dddddd",
					"name": "Border"
				}
			],
			"gradients": [
				{
					"slug": "primary-to-accent",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--accent) 100%)",
					"name": "Primary to Accent"
				},
				{
					"slug": "secondary-to-primary",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--primary) 100%)",
					"name": "Secondary to Primary"
				},
				{
					"slug": "light-to-dark",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--background) 0%, var(--wp--preset--color--background-alt) 100%)",
					"name": "Light to Dark"
				},
				{
					"slug": "dark-overlay",
					"gradient": "linear-gradient(180deg, rgba(35,40,45,0) 0%, rgba(35,40,45,0.8) 100%)",
					"name": "Dark Overlay"
				}
			],
			"duotone": [
				{
					"slug": "primary-and-background",
					"colors": ["#0073aa", "#ffffff"],
					"name": "Primary and Background"
				},
				{
					"slug": "secondary-and-background",
					"colors": ["#23282d", "#ffffff"],
					"name": "Secondary and Background"
				},
				{
					"slug": "accent-and-secondary",
					"colors": ["#00a0d2", "#23282d"],
					"name": "Accent and Secondary"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": ["px", "em", "rem", "%", "vw", "vh"],
			"spacingSizes": [
				{
					"slug": "10",
					"size": "0.25rem",
					"name": "1"
				},
				{
					"slug": "20",
					"size": "0.5rem",
					"name": "2"
				},
				{
					"slug": "30",
					"size": "1rem",
					"name": "3"
				},
				{
					"slug": "40",
					"size": "1.5rem",
					"name": "4"
				},
				{
					"slug": "50",
					"size": "2rem",
					"name": "5"
				},
				{
					"slug": "60",
					"size": "3rem",
					"name": "6"
				},
				{
					"slug": "70",
					"size": "4rem",
					"name": "7"
				},
				{
					"slug": "80",
					"size": "6rem",
					"name": "8"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, sans-serif",
					"slug": "system",
					"name": "System"
				},
				{
					"fontFamily": "Georgia, 'Times New Roman', Times, serif",
					"slug": "serif",
					"name": "Serif"
				},
				{
					"fontFamily": "'Courier New', Courier, monospace",
					"slug": "monospace",
					"name": "Monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.25rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "1.5rem",
					"name": "Extra Large"
				},
				{
					"slug": "xx-large",
					"size": "2rem",
					"name": "Huge"
				},
				{
					"slug": "xxx-large",
					"size": "3rem",
					"name": "Gigantic"
				}
			]
		},
		"custom": {
			"spacing": {
				"outer": "2rem"
			},
			"border": {
				"radius": "4px"
			},
			"transition": {
				"duration": "0.2s"
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border--radius)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"bottom": "0.75rem",
						"left": "1.5rem",
						"right": "1.5rem"
					}
				},
				"typography": {
					"fontWeight": "500"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary-dark)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--primary-dark)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary-dark)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "1.3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "1.4"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"left": "var(--wp--custom--spacing--outer)",
				"right": "var(--wp--custom--spacing--outer)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"width": "2px",
							"color": "var(--wp--preset--color--primary)"
						},
						"color": {
							"background": "transparent",
							"text": "var(--wp--preset--color--primary)"
						},
						":hover": {
							"color": {
								"background": "var(--wp--preset--color--primary)",
								"text": "var(--wp--preset--color--background)"
							}
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"color": "var(--wp--preset--color--primary)",
						"width": "4px"
					}
				},
				"spacing": {
					"padding": {
						"left": "1.5rem"
					}
				},
				"typography": {
					"fontStyle": "italic"
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--background-alt)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": "1rem"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)"
				}
			},
			"core/separator": {
				"color": {
					"text": "var(--wp--preset--color--border)"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
