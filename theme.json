{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": []
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1400px"
		},
		"spacing": {
			"customSpacingSize": false,
			"spacingSizes": [],
			"units": ["px", "rem", "%"]
		},
		"typography": {
			"customFontSize": false,
			"fontFamilies": [],
			"fontSizes": []
		},
		"blocks": {
			"core/button": {
				"color": {
					"custom": false
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "#ffffff",
			"text": "#333333"
		},
		"typography": {
			"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
			"fontSize": "16px",
			"lineHeight": "1.6"
		}
	}
}
